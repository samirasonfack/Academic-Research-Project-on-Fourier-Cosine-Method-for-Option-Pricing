 np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","    gamma = k * np.pi * (a_p - a) / (b - a)\n","    I_p = (val * ( ((-1)**k) *np.exp(b) - K * np.cos(gamma) + K * np.sqrt(val) * np.sin(gamma) )) / (1 + val)\n","    return I_p\n","\n","def pay_off(a,b):\n","\n","  if np.log(K) >= b:\n","    return 0\n","\n","  if np.log(K) <= a:\n","    return np.exp(b) - np.exp(a) - K*(b-a)\n","\n","  if a < np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","\n","    return np.exp(b) - np.exp(a_p) - K*(b-a_p)\n","\n","def sum(N):\n","  s = 0\n","  for i in range(1,N+1):\n","    s += F(i)*V(i)\n","  return s\n","\n","def price(N):\n","  return  np.exp(-r * delta_t) * ( (F(0)/2) * pay_off(a,b) + sum(N) )\n","\n","\n","\n","\n","\n"]},{"cell_type":"markdown","source":["<h1> Application to the Black-Scholes Model"],"metadata":{"id":"ZI5lEdxkr_Fd"}},{"cell_type":"code","source":["N_range = np.arange(5,200 , 1)  # De 10 à 500 avec un pas de 5\n","prices = np.array([price(N) for N in N_range])\n","\n","# Affichage du graphique\n","plt.figure(figsize=(10, 5))\n","plt.plot(N_range, prices, label=\"Prix de l'option\", color=\"blue\")\n","plt.xlabel(\"Nombre de termes N\")\n","plt.ylabel(\"Prix de l'option\")\n","plt.title(\"Convergence du prix de l'option avec la méthode COS\")\n","plt.grid()\n","plt.legend()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":520},"id":"UhOegE3vblOA","outputId":"af2863ed-2644-4693-f230-092604d0db14","executionInfo":{"status":"ok","timestamp":1747581360682,"user_tz":-60,"elapsed":603,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":22,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x500 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["from scipy.stats import norm\n","\n","# ✅ Prix avec Black-Scholes\n","def price_BS():\n","    d1 = (np.log(S_0 / K) + (r + (sigma**2) / 2) * T) / (sigma * np.sqrt(T))\n","    d2 = d1 - sigma * np.sqrt(T)\n","    return S_0 * norm.cdf(d1) - K * np.exp(-r * T) * norm.cdf(d2)\n","\n","# ✅ Calcul du prix exact avec Black-Scholes\n","price_exact = price_BS()\n","\n","# ✅ Étude de la convergence pour différentes valeurs de N\n","\n","prices = np.array([price(N) for N in N_range])\n","errors = np.abs(prices - price_exact)\n","\n","# ✅ Graphique de convergence\n","plt.figure(figsize=(12, 5))\n","\n","plt.subplot(1, 2, 1)\n","plt.plot(N_range, prices, label=\"Prix COS\", color=\"blue\")\n","plt.axhline(y=price_exact, color=\"red\", linestyle=\"--\", label=\"Prix Black-Scholes\")\n","plt.xlabel(\"Nombre de termes N\")\n","plt.ylabel(\"Prix de l'option\")\n","plt.title(\"Convergence du prix de l'option avec la méthode COS\")\n","plt.legend()\n","plt.grid()\n","\n","plt.subplot(1, 2, 2)\n","plt.plot(N_range, errors, label=\"Erreur absolue\", color=\"green\")\n","plt.xlabel(\"Nombre de termes N\")\n","plt.ylabel(\"Erreur\")\n","plt.title(\"Erreur d'estimation par rapport à Black-Scholes\")\n","plt.legend()\n","plt.grid()\n","\n","plt.tight_layout()\n","plt.show()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":465},"id":"w-Kz14NmdBlz","outputId":"32ac033b-9f90-4099-8ab3-a7cf02f9e987","executionInfo":{"status":"ok","timestamp":1747581361514,"user_tz":-60,"elapsed":830,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":23,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x500 with 2 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["<h1>Application to the Heston Model"],"metadata":{"id":"jv5zd8yJsWhl"}},{"cell_type":"code","source":["#param= [T , chi , rho , neta ,theta , Vo , x] rho négatif\n","#      = [0 ,  1   , 2 ,  3 ,    4,     5 ,  6]\n","def phi(u, param , beta , gamma, alpha ):\n","  return (param[1] * param[4] / param[3]**2 ) * ((beta - gamma)* T - 2*np.log( (1 - alpha * np.exp(-gamma * T)) / (1 - alpha) )) + 1j * u * r * param[0]\n","\n","def psi(u, param , beta , gamma , alpha ):\n","  return ((beta - gamma) / param[3]**2) * ((1 - np.exp( -gamma * T)) / (1 - alpha * np.exp(-gamma * T)))\n","\n","def phi_Heston(u, param):\n","  beta = param[1] - 1j * u * param[2] * param[3]\n","  gamma  = np.sqrt(beta**2 + param[3]**2 * (u**2 + 1j * u))\n","  alpha = (beta - gamma) / (beta + gamma)\n","\n","  return np.exp( phi(u, param , beta , gamma, alpha ) + 1j * u * param[6] + psi(u, param , beta , gamma , alpha ) * param[5])"],"metadata":{"id":"IPYV2X9fBBqu","executionInfo":{"status":"ok","timestamp":1747581361551,"user_tz":-60,"elapsed":18,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":24,"outputs":[]},{"cell_type":"code","source":["import scipy.fft\n","def prix_call_Carr_Madan(S0, K, T, r, params, N, eta=0.25, alpha=1.5):\n","    \"\"\"\n","     - eta : pas de discrétisation\n","    \"\"\"\n","\n","    # Grille de Fourier\n","    u = np.arange(N) * eta\n","    v = u - (N // 2) * eta  # Centrage\n","\n","    # transformée de Fourier\n","    psi = np.exp(-r * T) * phi_Heston(u - (alpha + 1) * 1j, params) / ((alpha + 1j * u) * (alpha + 1 + 1j * u))\n","\n","    # FFT\n","    fft_result = scipy.fft.fft(psi * np.exp(-1j * u * np.log(K))) * eta\n","\n","    # Récupération de la valeur pour K donné\n","    j = int(np.log(K / S0) / eta + N // 2)  # Indice correspondant à K\n","    call_price = np.exp(-alpha * np.log(K)) * np.real(fft_result[j]) / np.pi\n","\n","    return call_price\n","\n","# Exemple d'utilisation avec un modèle donné\n","S0, K, T, r = 100, 120, 1, 0.05\n","model_params = [1,1,-0.9,0.1,0.02,0.02,np.log(100)]  # Exemple pour Heston\n","import time\n","start = time.time()\n","price = prix_call_Carr_Madan(S0, K, T, r, model_params,500)\n","end = time.time()\n","print(end - start)\n","print(\"Prix du call :\", price)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LCTdiyY25KRo","outputId":"15021408-7d9b-4a2b-8e90-98b6ecdfa540","executionInfo":{"status":"ok","timestamp":1747581361612,"user_tz":-60,"elapsed":41,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":25,"outputs":[{"output_type":"stream","name":"stdout","text":["0.0011301040649414062\n","Prix du call : 0.900678853850764\n"]}]},{"cell_type":"code","source":["prices_heston = np.array([prix_call_Carr_Madan(S0,K,T,r,model_params,i) for i in N_range])\n","\n","# Affichage du graphique\n","plt.figure(figsize=(10, 5))\n","plt.plot(N_range, prices_heston, label=\"Prix de l'option avec Heston\", color=\"blue\")\n","plt.xlabel(\"Nombre de termes N\")\n","plt.ylabel(\"Prix de l'option\")\n","plt.title(\"Convergence du prix de l'option avec la méthode Carr Madan\")\n","plt.grid()\n","plt.legend()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":520},"id":"U8vwmfWbGtpC","outputId":"c431aeac-980e-4ff0-8ecb-20cee0590f7b","executionInfo":{"status":"ok","timestamp":1747581361673,"user_tz":-60,"elapsed":59,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":26,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x500 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["<h4> The bounds here were chosen according to the research paper (cumulants $c1$ and $c2$ respectively the mean and the variance of the log underlying asset price )"],"metadata":{"id":"W7xknvp89E9N"}},{"cell_type":"code","source":["\n","#param= [T , chi , rho , neta ,theta , Vo , x] rho négatif\n","#      = [0 ,  1   , 2 ,  3 ,    4,     5 ,  6]\n","def compute_c1(param) :\n","  c1 = param[6] + r * param[0] + (1 - np.exp(- param[0] * param[1])) * ( (param[4] - param[5]) / (2 * param[1]) )  - 0.5 * param[4] * param[0]\n","  return c1\n","\n","\n","def compute_c2(param):\n","    # Extract parameters\n","    T = param[0]   # T (time)\n","    lam = param[1] # lambda\n","    rho = param[2] # rho\n","    eta = param[3] # eta\n","    u_barre = param[4] # u_barre\n","    u0 = param[5]   # u0\n","\n","    # First term: eta * T * lambda * e^(-lambda * T) * (u0 - u_barre) * (8 * lambda * rho - 4 * eta)\n","    term2 = eta * T * lam * np.exp(-lam * T) * (u0 - u_barre) * (8 * lam * rho - 4 * eta)\n","\n","    # Second term: lambda * rho * eta * (1 - e^(-lambda * T)) * (16 * u_barre - 8 * u0)\n","    term3 = lam * rho * eta * (1 - np.exp(-lam * T)) * (16 * u_barre - 8 * u0)\n","\n","    # Third term: 2 * u_barre * lambda * T * (-4 * lambda * rho * eta + eta^2 + 4 * lambda^2)\n","    term4 = 2 * u_barre * lam * T * (-4 * lam * rho * eta + eta**2 + 4 * lam**2)\n","\n","    # Fourth term: eta^2 * ((u_barre - 2 * u0) * e^(-2 * lambda * T) + u_barre * (6 * e^(-lambda * T) - 7) + 2 * u0)\n","    term5 = eta**2 * ((u_barre - 2 * u0) * np.exp(-2 * lam * T) + u_barre * (6 * np.exp(-lam * T) - 7) + 2 * u0)\n","\n","    # Fifth term: 8 * lambda^2 * (u0 - u_barre) * (1 - e^(-lambda * T))\n","    term6 = 8 * lam**2 * (u0 - u_barre) * (1 - np.exp(-lam * T))\n","\n","    # Sum of all terms\n","    total_sum = term2 + term3 + term4 + term5 + term6\n","\n","    # Final result with the division by 8 * lambda^3\n","    c2 = (1 / (8 * lam**3)) * total_sum\n","\n","    return c2\n"],"metadata":{"id":"ZRQ3BsKBLja6","executionInfo":{"status":"ok","timestamp":1747581361703,"user_tz":-60,"elapsed":13,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":27,"outputs":[]},{"cell_type":"code","source":["c1_heston,c2_heston = compute_c1(model_params),compute_c2(model_params)\n","\n","a_heston = c1_heston - L*np.sqrt(c2_heston)\n","b_heston = c1_heston + L*np.sqrt(c2_heston)\n","# a_heston= -10\n","# b_heston= 10\n","\n","K=120\n","\n","def F_heston(k,a,b):\n","  alpha = np.exp( - (1j * k * a * np.pi)/(b - a))\n","  z = phi_Heston(k*np.pi/(b-a),model_params) * alpha\n","\n","  return (2/(b-a)) * np.real(z)\n","\n","\n","def V_heston(k,a,b):\n","\n","  val = ((b-a) / (k*np.pi))**2\n","  I = (val * ( ((-1)**k) *np.exp(b) - np.exp(a) )) / (1 + val)\n","\n","  if np.log(K) >= b:\n","    return 0\n","\n","  if np.log(K) <= a:\n","    return I\n","\n","  if a < np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","    gamma = k * np.pi * (a_p - a) / (b - a)\n","    I_p = (val * ( ((-1)**k) *np.exp(b) - K * np.cos(gamma) + K * np.sqrt(val) * np.sin(gamma) )) / (1 + val)\n","    return I_p\n","\n","def pay_off(a,b):\n","\n","  if np.log(K) >= b:\n","    return 0\n","\n","  if np.log(K) <= a:\n","    return np.exp(b) - np.exp(a) - K*(b-a)\n","\n","  if a < np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","\n","    return np.exp(b) - np.exp(a_p) - K*(b-a_p)\n","\n","def sum(N):\n","  s = 0\n","  for i in range(1,N+1):\n","    s += F_heston(i,a_heston,b_heston)*V_heston(i,a_heston,b_heston)\n","  return s\n","\n","def price_cos_heston(N):\n","  return  np.exp(-r * delta_t) * ( (F_heston(0,a_heston,b_heston)/2) * pay_off(a_heston,b_heston) + sum(N) )\n","\n","price_heston_final = price_cos_heston(500)\n","print(price_heston_final)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"IkSIAxfwXdCY","outputId":"1c29711f-5028-436f-e97f-c229d9789224","executionInfo":{"status":"ok","timestamp":1747581361738,"user_tz":-60,"elapsed":31,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":28,"outputs":[{"output_type":"stream","name":"stdout","text":["0.9147074895647026\n"]}]},{"cell_type":"code","source":["prices_heston_with_cos = np.array([price_cos_heston(i) for i in N_range])\n","\n","# Affichage du graphique\n","plt.figure(figsize=(10, 5))\n","plt.plot(N_range, prices_heston_with_cos, label=\"Prix de l'option avec Heston\", color=\"blue\")\n","plt.xlabel(\"Nombre de termes N\")\n","plt.ylabel(\"Prix de l'option\")\n","plt.title(\"Convergence du prix de l'option avec la méthode COS\")\n","plt.grid()\n","plt.legend()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":520},"id":"AgwnDgQpZxOK","outputId":"64f1b199-dbc4-4d0b-f450-3166eb89966b","executionInfo":{"status":"ok","timestamp":1747581362521,"user_tz":-60,"elapsed":780,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}}},"execution_count":29,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x500 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"markdown","source":["<h1> Application to the Lifted Heston Model"],"metadata":{"id":"MNt07_xDxw0h"}},{"cell_type":"code","source":["import numpy as np\n","np.seterr(divide='ignore', invalid='ignore')\n","import scipy.integrate as integrate\n","from scipy.special import gamma\n","\n","# Characteristic function of the Lifted Heston model see Slides 85-87\n","def Ch_Lifted_Heston(omega,S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint):\n","    # omega = argument of the ch. function\n","    # S0 = Initial price\n","    # rho,lamb,theta,nu,V0 = parameters Lifted Heston\n","    # N = number of factors in the model\n","    # rN = constant used to define weights and mean-reversions\n","    # alpha = H+1/2 where H is the Hurst index\n","    # T = maturity\n","    # M = number of steps in the time discretization to calculate ch. function\n","\n","    # to make sure we calculate ch. function and not moment gen. function\n","    i=complex(0,1)\n","    omega=i*omega\n","\n","    # Definition of weights and mean reversions in the approximation\n","    h=np.linspace(0,N-1,N)\n","    rpowerN=np.power(rN,h-N/2)\n","    # weights\n","    c=(rN**(1-alpha)-1)*(rpowerN**(1-alpha))/(gamma(alpha)*gamma(2-alpha))\n","    # mean reversions\n","    gammas=((1-alpha)/(2-alpha))*((rN**(2-alpha)-1)/(rN**(1-alpha)-1))*rpowerN\n","\n","    # Definition of the initial curve\n","    g = lambda t: V0+lamb*theta*np.dot(c/gammas,1-np.exp(-t*gammas))\n","\n","\n","    # Time steps for the approximation of psi\n","    delta = T/M;\n","    t=np.linspace(0,M,M+1)\n","    t = t * delta\n","\n","    # Function F\n","    F = lambda u,v : 0.5*(u**2-u)+(rho*nu*u-lamb)*v+.5*nu**2*v**2\n","\n","\n","    # Iteration for approximation of psi - see Slide 87\n","    psi=np.zeros((M+1,N),dtype=complex)\n","\n","    for k in range (1,M+1):\n","        psi[k,:] = (np.ones(N)/(1+delta*gammas))*(psi[k-1,:]+delta*F(omega,np.dot(c,psi[k-1,:]))*np.ones(N))\n","\n","\n","    # Invert g_0 to calculate phi - see Slide 87\n","    g_0=np.zeros((1,M+1))\n","\n","    for k in range(1,M+2):\n","        g_0[0,k-1]=g(T-t[k-1])\n","\n","\n","    Y=np.zeros((1,M+1),dtype=complex)\n","    phi=0\n","\n","    Y=F(omega,np.dot(c,psi.transpose()))*g_0\n","\n","\n","    # Trapezoid rule to calculate phi\n","    weights=np.ones(M+1)*delta\n","    weights[0]=delta/2\n","    weights[M]=delta/2\n","    phi=np.dot(weights,Y.transpose())\n","\n","    phi=np.exp(omega*np.log(S0)+phi+rint*T*omega)\n","\n","    return phi\n","\n","\n","# Computation of the price of Call option in the Lifted Heston model using Lewis formula\n","def Call_Price_Lifted_Heston(S0,K,T,rint,rho,lamb,theta,nu,V0,N,rN,alpha,M,alpha2,L):\n","    # S0 = Initial price\n","    # K,T = strike and maturity of the call option\n","    # rint = risk free rate\n","    # rho,lamb,theta,nu,V0 = parameters Lifted Heston\n","    # N = number of factors in the model\n","    # rN = constant used to define weights and mean-reversions\n","    # alpha = H+1/2 where H is the Hurst index\n","    # M = number of steps in the time discretization to calculate ch. function\n","    # alpha2 = damping factor in the Carr-Madan formula\n","    # L = truncation bound for the integral\n","\n","    # Lifted Heston characteristic function\n","    i=complex(0,1)\n","    phi=lambda omega:Ch_Lifted_Heston(omega,S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint)\n","\n","\n","    #Integrand in Carr-Madan formula - see notes on the Fourier transform fmla. (4.36)-(4.37)\n","    integrand=lambda omega:np.real((phi(omega-i*(alpha2+1))/(alpha2**2+alpha2-omega**2+i*(2*alpha2+1)*omega))*np.exp(-i*np.log(K)*omega))\n","\n","    # Pricing formula - Carr-Madan - see notes on the Fourier transform fmla. (4.36)-(4.37)\n","    I = integrate.quad(integrand,0,L)\n","    P=(np.exp(-rint*T-alpha2*np.log(K))/np.pi)*np.asarray(I)[0]\n","\n","    return P\n","\n","# Exemple d'utilisation avec un modèle donné\n","S0=1;rint=0.05;rho=-.7;lamb=.3;theta=.02;nu=.3;V0=.02;N=20;\n","rN=1+10*(N**(-.9));alpha=.6;M=100;L=1000;alpha2=1;\n","\n","T=1/26;\n","\n","K = 0.92\n","\n","\n","import time\n","start = time.time()\n","\n","P=Call_Price_Lifted_Heston(1,K,T,rint,rho,lamb,theta,nu,V0,N,rN,alpha,M,alpha2,L)\n","\n","end = time.time()\n","print(end - start)\n","print(\"Prix du call :\", P)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"1SAroifzxwfl","executionInfo":{"status":"ok","timestamp":1747581363812,"user_tz":-60,"elapsed":1232,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}},"outputId":"7146f546-a642-48b6-ca95-d8e68814ec45"},"execution_count":30,"outputs":[{"output_type":"stream","name":"stdout","text":["1.3060410022735596\n","Prix du call : 0.08216126759664405\n"]}]},{"cell_type":"code","source":["c1_lifted_heston = 2\n","c2_lifted_heston = 0.05\n","L_lifted_heston = 5\n","\n","\n","a_lifted_heston = c1_lifted_heston - L_lifted_heston*np.sqrt(c2_lifted_heston)\n","b_lifted_heston = c1_lifted_heston + L_lifted_heston*np.sqrt(c2_lifted_heston)\n","\n","\n","def F_lifted_heston(k,a,b,S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint):\n","  alpha = np.exp( - (1j * k * a * np.pi)/(b - a))\n","  z = Ch_Lifted_Heston(k*np.pi/(b-a),S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint) * alpha\n","\n","  return (2/(b-a)) * np.real(z)\n","\n","\n","def V_lifted_heston(k,a,b):\n","\n","  val = ((b-a) / (k*np.pi))**2\n","  I = (val * ( ((-1)**k) *np.exp(b) - np.exp(a) )) / (1 + val)\n","\n","  if np.log(K) >= b:\n","    return 0\n","\n","  if np.log(K) <= a:\n","    return I\n","\n","  if a < np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","    gamma = k * np.pi * (a_p - a) / (b - a)\n","    I_p = (val * ( ((-1)**k) *np.exp(b) - K * np.cos(gamma) + K * np.sqrt(val) * np.sin(gamma) )) / (1 + val)\n","    return I_p\n","\n","\n","def pay_off(a,b):\n","\n","  if np.log(K) >= b:\n","    return 0\n","\n","  if np.log(K) <= a:\n","    return np.exp(b) - np.exp(a) - K*(b-a)\n","\n","  if a < np.log(K) and np.log(K) < b:\n","    a_p = np.log(K)\n","\n","    return np.exp(b) - np.exp(a_p) - K*(b-a_p)\n","\n","\n","def sum(N):\n","  s = 0\n","  for i in range(1,N+1):\n","    s += F_lifted_heston(i,a_lifted_heston,b_lifted_heston,S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint)*V_lifted_heston(i,a_lifted_heston,b_lifted_heston)\n","  return s\n","\n","def price_cos_lifted_heston(N):\n","  return  np.exp(-rint * T) * ( (F_lifted_heston(0,a_lifted_heston,b_lifted_heston,S0,T,rho,lamb,theta,nu,V0,N,rN,alpha,M,rint)/2) * pay_off(a_lifted_heston,b_lifted_heston) + sum(N) )\n","\n","price_lifted_heston_final = price_cos_lifted_heston(500)\n","print(price_lifted_heston_final)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"D_CpkJisyO8b","executionInfo":{"status":"ok","timestamp":1747581372078,"user_tz":-60,"elapsed":8263,"user":{"displayName":"Lilyves TCHENANG","userId":"07021366458223829890"}},"outputId":"0ebba476-9485-4148-b6c9-7f58f2d34f39"},"execution_count":31,"outputs":[{"output_type":"stream","name":"stderr","text":["<ipython-input-30-0df08d990f5b>:53: ComplexWarning: Casting complex values to real discards the imaginary part\n","  g_0[0,k-1]=g(T-t[k-1])\n","<ipython-input-30-0df08d990f5b>:39: RuntimeWarning: overflow encountered in scalar power\n","  F = lambda u,v : 0.5*(u**2-u)+(rho*nu*u-lamb)*v+.5*nu**2*v**2\n","<ipython-input-30-0df08d990f5b>:39: RuntimeWarning: overflow encountered in square\n","  F = lambda u,v : 0.5*(u**2-u)+(rho*nu*u-lamb)*v+.5*nu**2*v**2\n","<ipython-input-30-0df08d990f5b>:59: RuntimeWarning: overflow encountered in multiply\n","  Y=F(omega,